{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "@ianvs/prettier-plugin-sort-imports"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"importOrder": [
		"^svelte(/.+)?",
		"^@sveltejs/.+",
		"^@melt-ui/.+",
		"<THIRD_PARTY_MODULES>",
		"<TYPES>^svelte/?",
		"<TYPES>^@sveltejs/.+",
		"<TYPES>^@melt-ui/.+",
		"<TYPES><THIRD_PARTY_MODULES>",
		"",
		"^utils$",
		"^ui/.+",
		"^[.]",
		"<TYPES>^utils$",
		"<TYPES>^ui/.+",
		""
	]
}
